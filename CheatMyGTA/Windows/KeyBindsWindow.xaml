<Window x:Class="CheatMyGTA.KeyBindsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheatMyGTA"
        mc:Ignorable="d"
        Title="KeyBindsWindow" Width="300" Height="400" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="0" HorizontalScrollBarVisibility="Auto" Margin="10,10,0,0">
            <StackPanel x:Name="StackPanel">
            </StackPanel>
        </ScrollViewer>

        <Separator Grid.Row="1"></Separator>
        <Label Grid.Row="2" Margin="10,0" x:Name="HelpLabel"/>


    </Grid>
</Window>
